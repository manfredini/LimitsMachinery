<!---
			IN THIS REGION ALL HISTO ARE 1 BIN HISTO
-->



<!DOCTYPE Channel  SYSTEM 'HistFactorySchema.dtd'>

  <Channel Name="region_C_tag" InputFile="./data/leplep_tag_emu.root" >
    <Data HistoName="ATLAS_data_LL_C_yield_FullBtag" HistoPath="abcd_FullBtag/" />

    <!-- Set the StatError type to Poisson.  Can also be Gaussian -->
    <StatErrorConfig RelErrorThreshold="0.05" ConstraintType="Poisson" /> 
<!--
		NO SIGNAL HERE
-->
    <Sample Name="ATLAS_Bkg_LL_Ztautau" HistoPath="abcd_FullBtag/" NormalizeByTheory="True" HistoName="ATLAS_Bkg_LL_Ztautau_C_yield_FullBtag">
      <StatError Activate="False" />  
    </Sample>

    <Sample Name="ATLAS_Bkg_LL_TT" HistoPath="abcd_FullBtag/" NormalizeByTheory="True" HistoName="ATLAS_Bkg_LL_TT_C_yield_FullBtag">
      <StatError Activate="False" />  
    </Sample>

    <Sample Name="ATLAS_Bkg_LL_Other" HistoPath="abcd_FullBtag/" NormalizeByTheory="True" HistoName="ATLAS_Bkg_LL_Other_C_yield_FullBtag">
      <StatError Activate="False" />  
    </Sample>

    <Sample Name="ATLAS_Bkg_QCD" HistoPath="abcd_FullBtag/" NormalizeByTheory="false" HistoName="unit_histo">  <!--unit_histo is just 1 bin histo with 1 event-->
      <StatError Activate="False" />  
      <NormFactor Name="N_qcd_B_tag" Val="50." Low="0." High="1000."  /> <!-- Number of events in region B to be fitted --> 

      <NormFactor Name="R_qcd_tag" Val="2." Low="0." High="10."  /> 	<!-- R_qcd factor that converts # events in region B to SR A, 
									can say also that brings SS to OS, to be fitted -->

      <NormFactor Name="R_qcd_bd_tag" Val="100." Low="30." High="1000."  /> 	<!-- R_qcd_bd factor that converts # events in region B to region D, 
									can say also that brings ISO to anti-ISO, to be fitted -->
    </Sample>
  </Channel>
